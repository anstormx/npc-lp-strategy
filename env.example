# Blockchain connection
RPC_URL=https://mainnet.base.org
PRIVATE_KEY=your_private_key_here

# Oracle settings
ONEINCH_FALLBACK_ENABLED=true  # Set to false to disable 1inch fallback for swaps

# Strategy parameters
CHECK_INTERVAL_SECONDS=20
RANGE_WIDTH_PERCENT=10
SLIPPAGE_PERCENT=1
POSITION_STEP=1
REBALANCE_THRESHOLD=0.5
MAX_TICK_DEVIATION=60
CYCLE_STEP_DELAY=3600
DYNAMIC_FEE_TIER=false  # Set to true to enable dynamic fee tier selection
CREATE_POOL_IF_NEEDED=false  # Set to true to allow creating pool if it doesn't exist

# Database settings (optional)
MONGO_URI=mongodb://localhost:27017
DB_NAME=base_lp_strategy